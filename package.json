{
    "name": "srcbin",
    "publisher": "dheerajpv",
    "displayName": "Sourcebin Uploader",
    "icon": "icon.png",
    "description": "Uploads Code Selection to SourceBin.",
    "version": "1.0.4",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dheerajpv/srcbin"
    },
    "keywords": [
        "bin",
        "sourcebin",
        "upload"
    ],
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "srcbin.upload",
                "title": "Upload to Bin"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/node-fetch": "^2.5.10",
        "@types/vscode": "^1.55.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "@sourcebin/linguist": "^0.0.3",
        "clipboardy": "^2.3.0",
        "node-fetch": "^2.6.1",
        "sourcebin": "^4.2.3"
    }
}
